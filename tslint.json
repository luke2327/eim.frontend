{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "object-literal-key-quotes": [true, "as-needed"],
        "align": [true, "parameters", "statements"],
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "member-access": false,
        "interface-name": false
    },
    "rulesDirectory": []
}